div.row
  div.small-12.columns.small-centered.lcm {{ board.lcm().value }}
    div.factorization
      span(ng-repeat="(k, v) in board.lcm().factorization") {{ k }}
        sup {{ v }}
        | 

div.row.board
  div.outer-square.small-4.columns(ng-repeat="square in board.squares track by $index")
    div.square
      div.square-content
        div.inner-square
          div.inner-square-content
            div.wrapper
              | {{ square.product() }}
              div.factorization
                span(ng-repeat="(k, v) in square.primes") {{ k }}
                  sup {{ v }}
                  | 
